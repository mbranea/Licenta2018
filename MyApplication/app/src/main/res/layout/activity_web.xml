<?xml version="1.0" encoding="utf-8"?>
<com.yarolegovich.mp.MaterialPreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.madalinbranea.myapplication.AndroidUI.MainActivity">


    <com.yarolegovich.mp.MaterialPreferenceCategory
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:mpc_title="Web development">

        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/javascript"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title="JavaScript" />
        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/html"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title="HTML " />
        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/css"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title="CSS" />
        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/premade"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title="Premade Tools" />

    </com.yarolegovich.mp.MaterialPreferenceCategory>

    <Button
        android:id="@+id/BackButtonWeb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:drawableLeft="@drawable/ic_save_black_24dp"

        />




</com.yarolegovich.mp.MaterialPreferenceScreen>