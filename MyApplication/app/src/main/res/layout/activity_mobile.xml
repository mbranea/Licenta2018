<?xml version="1.0" encoding="utf-8"?>
<com.yarolegovich.mp.MaterialPreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/screen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.madalinbranea.myapplication.AndroidUI.MainActivity">


    <com.yarolegovich.mp.MaterialPreferenceCategory
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:mpc_title="Android Programming">

        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/android_ui"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title="UI Design" />
        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/android_os"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title=" Android OS " />
        <com.yarolegovich.mp.MaterialRightIconPreference
            android:id="@+id/android_fs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:mp_icon="@drawable/ic_list_black_24dp"
            app:mp_icon_tint="@color/grayIconTint"
            app:mp_title="Android Filesystem" />


    </com.yarolegovich.mp.MaterialPreferenceCategory>

    <Button
        android:id="@+id/BackButtonMobile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:drawableLeft="@drawable/ic_save_black_24dp"

        />




</com.yarolegovich.mp.MaterialPreferenceScreen>